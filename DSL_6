}
}

}
} else
if ($AppType==Angular)
{
println("This is Angular space")
multibranchPipelineJob("$AppName1") {
    branchSources {
        git {
            id('12121212') // IMPORTANT: use a constant and unique identifier mahesh
            remote("$GitName")
            credentialsId('Git-ID')

        }
    }
    orphanedItemStrategy {
        discardOldItems {
            numToKeep(0)
        }
    }
}
