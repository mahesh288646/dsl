multibranchPipelineJob('example') {
    branchSources {
        git {
            id('12121212') // IMPORTANT: use a constant and unique identifier
            remote('https://github.com/mahesh288646/SampleMaven1.git')
            credentialsId('Git-ID')

        }
    }
    orphanedItemStrategy {
        discardOldItems {
            numToKeep(0)
        }
    }
}

buildPipelineView('example') {
    triggerOnlyLatestJob()
}